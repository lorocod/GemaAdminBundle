<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no" />
        <meta name="apple-mobile-web-app-capable" content="yes" />    
        <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
        
        <title>{% block title %}Welcome!{% endblock %}</title>
        
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />

        <link href="{{ asset("bundles/gemaadmin/css/bootstrap.min.css") }}" rel="stylesheet" />
        <link href="{{ asset("bundles/gemaadmin/css/bootstrap-responsive.min.css") }}" rel="stylesheet" />

        <link href="http://fonts.googleapis.com/css?family=Open+Sans:400italic,600italic,400,600" rel="stylesheet" />
        <link href="{{ asset("bundles/gemaadmin/css/font-awesome.min.css") }}" rel="stylesheet" />        

        <link href="{{ asset("bundles/gemaadmin/css/ui-lightness/jquery-ui-1.10.0.custom.min.css") }}" rel="stylesheet" />

        <link href="{{ asset("bundles/gemaadmin/css/base-admin-2.css") }}" rel="stylesheet" />
        <link href="{{ asset("bundles/gemaadmin/css/base-admin-2-responsive.css") }}" rel="stylesheet" />

        <link href="{{ asset("bundles/gemaadmin/css/pages/dashboard.css") }}" rel="stylesheet" />   

        <link href="{{ asset("bundles/gemaadmin/css/custom.css") }}" rel="stylesheet" />
        {% block stylesheets %}{% endblock %}
        <!-- Le HTML5 shim, for IE6-8 support of HTML5 elements -->
        <!--[if lt IE 9]>
          <script src="http://html5shim.googlecode.com/svn/trunk/html5.js"></script>
        <![endif]-->

        
    </head>
    <body>
        {% block navbar %}{% render controller('GemaAdminBundle:Ejemplo\\Barra:navbar') %}{% endblock %}
        {% block subnavbar %}{% render controller('GemaAdminBundle:Ejemplo\\Barra:subnavbar') %}{% endblock %}
        <div class="contenido">
        {% block body %}{% endblock %}
        </div>
        {% render controller('GemaAdminBundle:Ejemplo\\Barra:extra') %}
        {% render controller('GemaAdminBundle:Ejemplo\\Barra:footer') %}

        {% block jscompleto %}
        <script src="{{ asset("bundles/gemaadmin/js/libs/jquery-1.8.3.min.js") }}"></script>
        <script src="{{ asset("bundles/gemaadmin/js/libs/jquery-ui-1.10.0.custom.min.js") }}"></script>
        <script src="{{ asset("bundles/gemaadmin/js/libs/bootstrap.min.js") }}"></script>


        {% block javascripts %}{% endblock %}
        <script src="{{ asset("bundles/gemaadmin/js/Application.js") }}"></script>

        <!-- javascripts extras de cada pagina -->
        {% block app_extra %}{% endblock %}
        {% endblock %}
  </body>
</html>
